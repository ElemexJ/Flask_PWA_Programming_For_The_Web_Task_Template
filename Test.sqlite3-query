SELECT * 
FROM UserInfomation;

SELECT Username, Password 
FROM UserInfomation;

SELECT UserID, Age 
FROM UserInfomation 
WHERE Age > 29;

SELECT UserID, Username, Email 
FROM UserInfomation 
WHERE Email LIKE '%gmail.com';

SELECT UserID, Username, Password
FROM UserInfomation
WHERE Password NOT LIKE '%!%'
AND Password NOT LIKE '%&%'
AND Password NOT LIKE '%$%'
AND Password NOT LIKE '%^%'
AND Password NOT LIKE '%*%'
AND Password NOT LIKE '%(%'
AND Password NOT LIKE '%)%'
AND Password NOT LIKE '%@%'
AND Password NOT LIKE '%#%'
AND Password NOT LIKE '%?%'
AND Password NOT LIKE '%/%'
AND Password NOT LIKE '%<%'
AND Password NOT LIKE '%>%'
AND Password NOT LIKE '%{%'
AND Password NOT LIKE '%}%'
AND Password NOT LIKE '%[%'
AND Password NOT LIKE '%]%'
AND Password NOT LIKE '%;%';